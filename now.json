{
  "env": {
    "API_KEY_STATIONS": "@api_key_stations",
    "API_KEY_TRIPS": "@api_key_trips",
    "MAPBOX_TOKEN": "@mapbox_token"
  },
  "build": {
    "env": {
      "API_KEY_STATIONS": "@api_key_stations",
      "API_KEY_TRIPS": "@api_key_trips",
      "MAPBOX_TOKEN": "@mapbox_token"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404" }
  ]
}